{
  "name": "SyncKit",
  "version": "0.4",
  "summary": "CloudKit synchronization for your Core Data model.",
  "description": "SyncKit automates the process of synchronizing your Core Data models using CloudKit. It can easily be plugged into (and removed from) your existing stack.",
  "homepage": "https://github.com/mentrena/SyncKit",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Manuel": "manuel@mentrena.com"
  },
  "source": {
    "git": "https://github.com/mentrena/SyncKit.git",
    "tag": "0.4"
  },
  "platforms": {
    "ios": "8.0",
    "osx": "10.11"
  },
  "subspecs": [
    {
      "name": "CoreData",
      "public_header_files": [
        "SyncKit/Classes/QSSynchronizer/*.h",
        "SyncKit/Classes/CoreData/*.h"
      ],
      "source_files": [
        "SyncKit/Classes/QSSynchronizer/*.{h,m}",
        "SyncKit/Classes/CoreData/*.{h,m}"
      ],
      "resources": "SyncKit/Classes/CoreData/*.xcdatamodeld",
      "frameworks": [
        "CoreData",
        "CloudKit"
      ]
    },
    {
      "name": "Realm",
      "public_header_files": [
        "SyncKit/Classes/QSSynchronizer/*.h",
        "SyncKit/Classes/Realm/*.h"
      ],
      "source_files": [
        "SyncKit/Classes/QSSynchronizer/*.{h,m}",
        "SyncKit/Classes/Realm/*.{h,m}"
      ],
      "frameworks": "CloudKit",
      "dependencies": {
        "Realm": [

        ]
      }
    }
  ]
}
